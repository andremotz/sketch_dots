class Datahandler {
  Datahandler() {
  
  }
  
  private int gridSize;
  private int pointSize;
  private boolean brightnessMode;
  private int imgWidth;
  private int imgHeight;
  private boolean recordPDF;
  private int windowWidth;
  private int windowHeight;
  
  int getGridSize() {
     return gridSize;
  }
  
  void setGridSize(int _gridSize) {
      gridSize = _gridSize;
  }
  
  int getPointSize() {
    return pointSize;
  }
  
  void setPointSize(int _pointSize) {
    pointSize = _pointSize;
  }
  
  boolean getBrightnessMode() {
    return brightnessMode;
  }
  
  void setBrightnessMode(boolean _brightnessMode) {
    brightnessMode = _brightnessMode;
  }
  
  int getImgWidth(){
    return imgWidth;
  }
  
  void setImgWidth(int _imgWidth) {
    imgWidth = _imgWidth;
  }
  
  int getImgHeight(){
    return imgHeight;
  }
  
  void setImgHeight(int _imgHeight) {
    imgHeight = _imgHeight;
  }
  
  boolean getRecordPDF() {
    return recordPDF;
  }
  
  void setRecordPDF(boolean _recordPDF) {
    recordPDF = _recordPDF;
  }
  
  int getWindowWidth() {
    return windowWidth;
  }
  
  void setWindowWidth(int _windowWidth) {
    windowWidth = _windowWidth;
  }
  
  int getWindowHeight() {
    return windowHeight;
  }
  
  void setWindowHeight(int _windowHeight) {
    windowHeight = _windowHeight;
  }
  
  
    
    
}
